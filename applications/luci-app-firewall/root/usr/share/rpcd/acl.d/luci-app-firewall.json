{
	"luci-app-firewall": {
		"description": "Grant access to firewall configuration",
		"read": {
			"file": {
				"/etc/firewall.user": [ "read" ]
			},
			"ubus": {
				"file": [ "read" ],
				"luci": [ "getConntrackHelpers", "getWED" ]
			},
			"uci": [ "firewall", "mtkhnat" ]
		},
		"write": {
			"file": {
				"/etc/firewall.user": [ "write" ],
				"/sbin/mtkhnat": [ "exec" ]
			},
			"ubus": {
				"file": [ "write" ],
				"luci": [ "setWED" ]
			},
			"uci": [ "firewall", "mtkhnat" ]
		}
	}
}
